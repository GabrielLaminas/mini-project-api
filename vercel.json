{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/certificado"
    },
    {
      "source": "/(.*)",
      "destination": "/validacao"
    }
  ]
}