{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [ 
    {
      "src": "/(.*)",
      "methods": [
        "POST",
        "GET"
      ],
      "dest": "/"
    }
  ],
  "headers": [
    {
      "src": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}